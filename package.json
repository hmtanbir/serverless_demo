  "scripts": {
    "serverless": "serverless",
    "build": "webpack --config webpack.config.js",
    "nodeploy": "serverless deploy -n",
    "deploy-dev": "serverless deploy --stage=dev --aws-profile=snr-dev",
    "func-dev": "serverless deploy function -f todo --stage=dev --aws-profile=snr-dev",
    "offline": "yarn serverless offline --env=dev --aws-profile=snr-dev",
    "remov": "serverless remove --stage=dev --aws-profile=snr-dev",
    "lint": "tslint \"src/**/*.ts\" --force",
    "test:serverless": "npm run build && serverless invoke local -f Homeork -p test/addHomework.json -s dev",
    "integration-tests": "mocha --timeout 40000 --exit test/integrationTests.js",
    "test": "jest",
    "print": "yarn serverless print --apiKey myapi-key --env=dev --stage=test --aws-profile=snr-dev",
    "installplugin": "serverless plugin install -n serverless-deployment-bucket"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^26.6.3",
    "serverless-deployment-bucket": "^1.6.0",
